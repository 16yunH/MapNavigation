" v-model.number=\"goalId\" placeholder=\"Enter goal node ID\">\r\n      <button @click=\"findShortestPath\">Find Shortest Path</button>\r\n    </div>\r\n    <div class=\"map\" ref=\"map\"></div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport L from 'leaflet';\r\nimport 'leaflet/dist/leaflet.css';\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      map: null,\r\n      startId: null,\r\n      goalId: null,\r\n      pathLayer: null,\r\n      nodes: {}\r\n    };\r\n  },\r\n  async created() {\r\n    try {\r\n      const response = await axios.get('http://localhost:8081/nodes');\r\n      this.nodes = response.data;\r\n    } catch (error) {\r\n      console.error('Error fetching nodes:', error);\r\n    }\r\n  },\r\n  mounted() {\r\n    this.initMap();\r\n    this.resizeMap();\r\n    window.addEventListener('resize', this.resizeMap);\r\n  },\r\n  beforeUnmount() {\r\n    window.removeEventListener('resize', this.resizeMap);\r\n  },\r\n  methods: {\r\n    initMap() {\r\n      this.map = L.map(this.$refs.map).fitBounds([\r\n        [28.2817100, 117.1911600],\r\n        [28.3225200, 117.2551900]\r\n      ]);\r\n\r\n      L.tileLayer('https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png', {\r\n        attribution: 'Map data &copy; <a href=\"https://www.openstreetmap.org/\">OpenStreetMap</a> contributors',\r\n        maxZoom: 18\r\n      }).addTo(this.map);\r\n    },\r\n    resizeMap() {\r\n      this.$nextTick(() => {\r\n        this.map.invalidateSize();\r\n      });\r\n    },\r\n    async findShortestPath() {\r\n      if (!this.startId || !this.goalId) {\r\n        alert('Please enter both start and goal node IDs.');\r\n        return;\r\n      }\r\n\r\n      try {\r\n        const response = await axios.post('http://localhost:8081/shortest_path', {\r\n          startId: this.startId,\r\n          goalId: this.goalId\r\n        });\r\n\r\n        if (this.pathLayer) {\r\n          this.map.removeLayer(this.pathLayer);\r\n        }\r\n\r\n        const path = response.data.path;\r\n        const coordinates = path.map(id => {\r\n          const node = this.nodes[id];\r\n          return [node.lat, node.lon];\r\n        });\r\n\r\n        this.pathLayer = L.polyline(coordinates, {color: 'blue'}).addTo(this.map);\r\n        this.map.fitBounds(this.pathLayer.getBounds());\r\n      } catch (error) {\r\n        console.error('Error finding shortest path:', error);\r\n        alert('An error occurred while finding the shortest path.');\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n.map-container {\r\n  display: flex;\r\n  flex-direction: column;\r\n  height: 100vh;\r\n}\r\n\r\n.map {\r\n  flex: 1;\r\n}\r\n\r\n.controls {\r\n  padding: 10px;\r\n}\r\n</style>"],"mappings":";;EACOA,KAAK,EAAC;AAAe;;EACnBA,KAAK,EAAC;AAAU;;EAKhBA,KAAK,EAAC,KAAK;EAACC,GAAG,EAAC;;;uBANvBC,mBAAA,CAOM,OAPNC,UAOM,GANJC,mBAAA,CAIM,OAJNC,UAIM,G,gBAHJD,mBAAA,CAAgF;IAAzEE,IAAI,EAAC,QAAQ;IAH1B,uBAAAC,MAAA,QAAAA,MAAA,MAAAC,MAAA,IAG2CC,KAAA,CAAAC,OAAO,GAAAF,MAAA;IAAEG,WAAW,EAAC;iDAArBF,KAAA,CAAAC,OAAO,E;IAAfE,MAAM,EAAd;EAAwB,E,oBAC7CR,mBAAA,CAA8E;IAAvEE,IAAI,EAAC,QAAQ;IAJ1B,uBAAAC,MAAA,QAAAA,MAAA,MAAAC,MAAA,IAI2CC,KAAA,CAAAI,MAAM,GAAAL,MAAA;IAAEG,WAAW,EAAC;iDAApBF,KAAA,CAAAI,MAAM,E;IAAdD,MAAM,EAAd;EAAuB,E,IAC5CR,mBAAA,CAA6D;IAApDU,OAAK,EAAAP,MAAA,QAAAA,MAAA,UAAAQ,IAAA,KAAEC,QAAA,CAAAC,gBAAA,IAAAD,QAAA,CAAAC,gBAAA,IAAAF,IAAA,CAAgB;KAAE,oBAAkB,E,GAEtDX,mBAAA,CAAiC,OAAjCc,UAAiC,8B","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}